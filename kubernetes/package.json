{
  "name": "trn-cdk8s-components",
  "version": "0.1.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc --build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "watch": "tsc --build -w",
    "test": "jest",
    "build": "yarn compile && yarn synth",
    "upgrade": "yarn add cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "yarn add cdk8s@next cdk8s-cli@next"
  },
  "engines": {
    "node": ">=20.10"
  },
  "dependencies": {
    "cdk8s": "^2.68.22",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cdk8s-cli": "^0.33.0-pre.713769883c9b4fe0ec443d0e776155d720b65cb6",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}